<?xml version="1.0" ?>

<mujoco model="frank_robot">
    <statistic extent="2" meansize=".05"/>
    <compiler angle="radian" coordinate="local" meshdir="frank-robot/meshes/"/>
    <size nconmax="50" njmax="500" nstack="50000"/>

    <!-- -->

    <option timestep="0.001" solver="Newton" iterations="30" tolerance="1e-10" jacobian="sparse" cone="pyramidal"/>
    <option>
        <flag gravity="enable"/>
    </option>

    <!-- -->

    <visual>
        <rgba haze="0.15 0.25 0.35 1"/>
        <quality shadowsize="2048"/>
        <map stiffness="700" shadowscale="0.5" fogstart="10" fogend="15" zfar="40" haze="0.3"/>
    </visual>

    <!-- -->

    <asset>
        <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="512"/>
        <texture name="texplane" type="2d" builtin="checker" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2"
            width="512" height="512" mark="cross" markrgb=".8 .8 .8"/>

        <material name="matplane" reflectance="0.3" texture="texplane" texrepeat="1 1" texuniform="true"/>

        <include file="frank-robot/frank-robot-assets.xml"/>
    </asset>

    <!-- -->

    <worldbody>
        <geom name="ground" type="plane" size="0 0 1" pos="0 0 0" quat="1 0 0 0" material="matplane" condim="1"/>

        <light directional="true" diffuse=".4 .4 .4" specular="0.1 0.1 0.1" pos="0 0 5.0" dir="0 0 -1" castshadow="true"/>
        <light directional="true" diffuse=".6 .6 .6" specular="0.2 0.2 0.2" pos="0 0 4" dir="0 0 -1"/>

        <camera name="isomestric_camera" pos="0.8 -0.6 1.0" xyaxes="0.707 0.707 0 -0.5 0.5 0.707"/>

        <body name="frank_robot_base" pos="0 0 0" euler="0 0 -1.57">
            <include file="frank-robot/frank-robot-collision.xml"/>
        </body>
    </worldbody>

    <!-- -->

    <actuator>
        <include file="frank-robot/frank-robot-actuators.xml"/>
    </actuator>
</mujoco>
