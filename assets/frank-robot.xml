<?xml version="1.0" ?>

<mujoco model="frank_robot">
    <compiler angle="radian" coordinate="local" meshdir="frank-robot/meshes/"/>

    <option>
        <flag gravity="enable"/>
    </option>

    <default>
        <default class="visual">
            <geom contype="0" conaffinity="0"/>
        </default>

        <default class="collision">
            <geom contype="1" conaffinity="1" friction=".6 0.005 0.0001"/>
        </default>
    </default>

    <option solver="CG" tolerance="1e-6"/>

    <size nconmax="50" njmax="500" nstack="50000"/>

    <include file="./scene.xml"/>

    <asset>
        <include file="frank-robot/frank-robot-assets.xml"/>
    </asset>

    <worldbody>
        <body name="frank_robot_base" pos="0 0 0" euler="0 0 -1.57">
            <include file="frank-robot/frank-robot-collision.xml"/>
        </body>
    </worldbody>

    <actuator>
        <include file="frank-robot/frank-robot-actuators.xml"/>
    </actuator>
</mujoco>
